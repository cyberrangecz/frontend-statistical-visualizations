{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./projects/trainings-statistical-visualizations",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2020",
        "module": "es2020",
        "lib": [
            "es2018",
            "dom"
        ],
        "paths": {
            "@cyberrangecz-platform/statistical-visualizations/barchart": [
                "./barchart/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/bubblechart": [
                "./bubblechart/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/clustering-wrapper": [
                "./clustering-wrapper/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/combined-diagram": [
                "./combined-diagram/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/scatterplot": [
                "./scatterplot/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/filters": [
                "./filters/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/statistical-viz": [
                "./statistical-viz/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/internal": [
                "./internal/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/scatter-clusters-wrapper": [
                "./scatter-clusters-wrapper/src/public_api.ts"
            ],
            "@cyberrangecz-platform/statistical-visualizations/radar-chart-wrapper": [
                "./radar-chart-wrapper/src/public_api.ts"
            ]
        }
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "strictInjectionParameters": true
    }
}
